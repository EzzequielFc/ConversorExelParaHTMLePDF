<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tabela</title>
</head>
<body>
    <table>
        <thead> <!-- Cabeçalho de tabela -->
            <% header.forEach(col =>{ %>
                <th> <%= col %> </th> <!-- Coluna do cabeçalho -->
            <% }) %>
        </thead>
        <tbody> <!-- Corpo da tabela -->
            <% rows.forEach(row =>{ %>
                <tr><!-- Linha de tabela -->
                    <% row.forEach(element => { %>
                        <td><%= element %></td>
                    <% }) %>    
                </tr> 
            <% }) %>            
        </tbody>
    </table>
</body>
</html>